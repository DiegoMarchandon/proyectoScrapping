{
    "name": "directorio-proyecto/mi-proyecto",
    "description": "directorio que almacenará la gestión de Composer",
    "version": "1.0.0",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "App\\":"src/",
            "Conector\\":"../../Modelo/conector/",
            "Clase\\":"../../Modelo/",
            "Controlador\\":"../../Controlador/"
        }
    },
    "autoload-dev": {
        "psr-4": { "App\\Tests\\":"tests/"}
    },
    "require": {
        "php-webdriver/webdriver": "^1.8"
    },
    "minimum-stability": "dev",
    "require-dev": {
        "phpunit/phpunit":"^9.1",
        "dbrekelmans/bdi": "dev-main",
        "symfony/panther": "^2.1"
    }
}
